@model ErrorViewModel

<div class="container">
    <h1 class="w3-jumbo w3-animate-top w3-center"><code>@Model.StatusCode</code></h1>
    <hr class="w3-border-white w3-animate-left" style="margin:auto;width:50%">
    <h3 class="w3-center w3-animate-right">@Model.ErrorMessage</h3>
    <h3 class="w3-center w3-animate-zoom">🚫🚫🚫🚫</h3>
</div>
